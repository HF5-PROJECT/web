---
import AdminHeader from "./AdminHeader.astro";
import Main from "./Main.astro";

export interface Props {
    title: string;
    user: Object;
}

const { title, user } = Astro.props;
---

<Main title={title}>
    <body>
        <AdminHeader user={user}>
            <slot />
        </AdminHeader>
    </body>
</Main>
